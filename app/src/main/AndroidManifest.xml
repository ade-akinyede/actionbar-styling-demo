<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidplaybook.showcasedemo">

    <!-- {@see http://developer.android.com/tools/projects/index.html#mipmap} for mipmap usage -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.androidplaybook.actionBarStylingDemo.ParentActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.androidplaybook.actionBarStylingDemo.ChildActivity"
            android:theme="@style/AppTheme">
        </activity>


    </application>

</manifest>